<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wgn.website.sys.mapper.ClavMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.wgn.website.sys.entity.ClavEntity">
        <result column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="title" property="title"/>
        <result column="part" property="part"/>
        <result column="author" property="author"/>
        <result column="upload_time" property="uploadTime"/>
        <result column="reply_num" property="replyNum"/>
        <result column="download_num" property="downloadNum"/>
        <result column="url" property="url"/>
        <result column="bt_path" property="btPath"/>
        <result column="status" property="status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_time,
        title, part, author, upload_time, reply_num, download_num, url, bt_path, status
    </sql>

</mapper>
